/* Typography */
h1:not(.modal-title), h2:not(.modal-title), h3:not(.modal-title), h4:not(.modal-title), h5:not(.modal-title), h6:not(.modal-title) {
  color: #1D1F21;
  margin-top: 0; }

h1 a,
h2 a,
h3 a,
h4 a,
h5 a,
h6 a {
  color: #1D1F21; }

h1 small,
h2 small,
h3 small,
h4 small,
h5 small,
h6 small {
  color: #222222;
  font-size: 60%; }

h1, .h1 {
  font-size: 2.8rem;
  font-weight: 300;
  line-height: 1.1;
  letter-spacing: -0.125rem; }

h2, .h2 {
  font-size: 2.4rem;
  font-weight: 300;
  line-height: 1.1;
  letter-spacing: -0.0625rem; }

h3, .h3 {
  font-size: 1.5rem;
  font-weight: 300;
  line-height: 1.4; }

h4, .h4 {
  font-size: 1.375rem;
  font-weight: 300;
  line-height: 1.4; }

h5, .h5 {
  font-size: 1.25rem;
  font-weight: 300;
  line-height: 1.4; }

h6, .h6 {
  font-size: 1.125rem;
  font-weight: 300;
  line-height: 1.1; }

@media (min-width: 992px) {
  h1, .h1 {
    font-size: 4.8rem;
    font-weight: 300;
    line-height: 1.1;
    letter-spacing: -0.25rem; }
  h2, .h2 {
    font-size: 4rem;
    font-weight: 300;
    line-height: 1.1;
    letter-spacing: -0.125rem; }
  h3, .h3 {
    font-size: 1.6rem;
    font-weight: 300;
    line-height: 1.4; }
  h4, .h4 {
    font-size: 1.5rem;
    font-weight: 300;
    line-height: 1.4; }
  h5, .h5 {
    font-size: 1.375rem;
    font-weight: 300;
    line-height: 1.4; }
  h6, .h6 {
    font-size: 1.25rem;
    font-weight: 300;
    line-height: 1.1; } }

h1.panel-title {
  letter-spacing: 0 !important; }

code {
  background-color: rgba(0, 0, 0, 0.8);
  color: #FFFFFF;
  border-radius: .2rem;
  padding: .1rem .3rem .2rem; }

em,
li {
  font-size: 1.5rem;
  font-weight: 500;
  color: #1D1F21; }
  em > a,
  li > a {
    color: #1D1F21; }
    em > a:hover,
    li > a:hover {
      color: #0c0d0e; }

label {
  font-size: 1rem;
  font-weight: 500 !important;
  color: #1D1F21;
  margin-bottom: .25rem; }

.label-text {
  font-weight: 400; }

i {
  line-height: 1; }

/* Paragraph */
p {
  font-size: 16px;
  font-weight: 400;
  color: #1D1F21;
  line-height: 1.4;
  margin-top: 0;
  margin-bottom: 1rem; }

/* Links */
a {
  color: #222222;
  text-decoration: none; }
  a:focus, a:hover {
    color: #0c0d0e;
    text-decoration: none; }
  a:focus {
    outline: none; }

.uppercase {
  text-transform: uppercase; }

.letter-space {
  letter-spacing: 2px; }

.big-letter-space {
  letter-spacing: 6px; }

