.video-list .sp-video{
    position:relative;
    width:30%;
    padding-bottom:30%;
    height:0;
    color:#222;
    text-align:center;
    transition:background-color 200ms ease-out;
    display:table
}
.video-list .sp-video .video-content{
    position:absolute;
    bottom:0;
    margin-bottom:10%
}
.video-list .sp-video .video-content p{
    margin-bottom:0
}
.video-list .sp-video .video-content h3{
    font-family:Inter,"Helvetica Neue",Helvetica,Arial,sans-serif;
    font-weight:900;
    font-size:1.75em;
    line-height:.9;
    font-style:italic;
    text-transform:uppercase;
    color:#fff;
    background-color:rgba(243,130,61,.77);
    letter-spacing:0;
    padding-top:.15em;
    padding-bottom:.15em
}
.video-list .sp-video .video-content .play-button{
    width:20%;
    margin:1em auto
}
.video-list .sp-video .video-content .description{
    line-height:1.5em;
    font-size:14px;
    width:75%;
    margin:0 auto
}
.video-list .sp-video .video-content .speaker{
    margin-top:10px;
    font-family:Inter,"Helvetica Neue",Helvetica,Arial,sans-serif;
    font-weight:600;
    text-transform:uppercase
}
@media (min-width:768px){
    .video-list .sp-video::after{
        position:absolute;
        content:"";
        background-size:cover;
        top:0;
        left:0;
        right:0;
        bottom:0;
        z-index:-1
    }
}
