    .video-list .sp-video:not(.inactive):hover{
        background-color:#F3823D;
        cursor:pointer
    }
    .video-list .sp-video:not(.inactive):hover .play-button,.video-list .sp-video:not(.inactive):hover p{
        visibility:visible
    }
    .video-list .sp-video .play-button,.video-list .sp-video p{
        visibility:hidden
    }
}
